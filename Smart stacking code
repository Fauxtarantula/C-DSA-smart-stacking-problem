#include <iostream>
#include <fstream>
#include <string>
#include <vector>

using namespace std;

void readFileFun() {

	ifstream inFile;
	inFile.open("list.txt");

	//error check
	if (inFile.fail()) {
		cerr << "Error Opening File" << endl;
		exit(1);
	}
	int count = 0;
	int counter = 12;
	string box;
	string box1;
	vector<int> numvect;
	vector<string> boxvect;

	for (; !inFile.eof(); count++) {
		getline(inFile, box, '\n');
		boxvect.push_back(box);
	}
	for (int n = 0; n < boxvect.size(); n++) {

		char cha = boxvect[n].at(12);
		box1 = cha;
		int m = stoi(box1);
		numvect.push_back(m);
		cout << numvect[n] << endl;
	}
	
}


void main() {
	readFileFun();

}	
