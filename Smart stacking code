#include <iostream>
#include <fstream>
#include <string>

using namespace std;

void readFileFun() {

	ifstream inFile;
	inFile.open("input.txt");

	//error check
	if (inFile.fail()) {
		cerr << "Error Opening File" << endl;
		exit(1);
	}

	string box;
	int count = 0;
	//Read a file until reach end
	while (!inFile.eof()){
		inFile >> box;
		count++;
	}
	cout << count << "items found" << endl;
}

void main() {
	readFileFun();

}

#include <iostream>
#include <fstream>
#include <string>
#include <vector>

using namespace std;

void readFileFun() {

	ifstream inFile;
	inFile.open("list.txt");

	//error check
	if (inFile.fail()) {
		cerr << "Error Opening File" << endl;
		exit(1);
	}
	int count = 0;
	string box;
	vector<string> boxvect;
	while (!inFile.eof()) {
		count++;
		for (int i = 0; i < count; i++) {
			getline(inFile, box, '\n');
			boxvect.push_back(box);
			}
		}
		for (int n = 0; n < boxvect.size(); n++) {
		cout << boxvect[n] << endl;
		}
	}

void main() {
	readFileFun();

}
////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////


#include <iostream>
#include <fstream>
#include <string>
#include <vector>

using namespace std;

void readFileFun() {

	ifstream inFile;
	inFile.open("list.txt");

	//error check
	if (inFile.fail()) {
		cerr << "Error Opening File" << endl;
		exit(1);
	}
	int count = 0;
	string box;
	vector<string> boxvect;

		count++;
		for (;!inFile.eof() ; count++) {
			getline(inFile, box, '\n');
			boxvect.push_back(box);
		}
	
	for (int n = 0; n < boxvect.size(); n++) {
		cout << boxvect[n] << endl;
	}
	
}

void main() {
	readFileFun();

}
