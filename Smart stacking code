#include <iostream>
#include <fstream>
#include <string>
#include <vector>

using namespace std;

void readFileFun() {

	ifstream inFile;
	inFile.open("list.txt");

	//error check
	if (inFile.fail()) {
		cerr << "Error Opening File" << endl;
		exit(1);
	}
	int count = 0;
	string box;
	vector<string> boxvect;

		count++;
		for (;!inFile.eof() ; count++) {
			getline(inFile, box, '\n');
			boxvect.push_back(box);
		}
	
	for (int n = 0; n < boxvect.size(); n++) {
		cout << boxvect[n] << endl;
	}
	
}

void main() {
	readFileFun();

}
